= simple_form_for([:admins, @application]) do |f|
  = f.error_notification

  .form-inputs
    = f.association :contributors, :prompt => "Katkıcı Seçiniz"
    = f.input :cname_domain
    = f.input :name
    = f.input :twitter_username
    = f.input :facebook_page

  .form-actions
    = f.button :submit
