= simple_form_for([:admins, @application]) do |f|
  = f.error_notification

  .form-inputs
    = f.input :owner_name, :input_html => {data: {autocomplete_source: auto_admins_contributors_path}, :value => ""}
    = f.input :cname_domain
    = f.input :name
    = f.input :logo, :as => :file
    = f.input :twitter_username
    = f.input :facebook_page

  .form-actions
    = f.button :submit
