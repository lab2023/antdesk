= title t('contributors')

- content_for :toolbar_left do
  = link_to(admins_dashboard_index_path , class: 'btn', rel: 'tooltip', title: t('tooltips.back')) do
    %i.icon-arrow-left

%table.table.table-striped.table-bordered.table-condensed
  %tr
    %th
      = t('activerecord.attributes.contributor.name')
    %th
      = t('email')
  - @contributors.each do |contributor|
    %tr
      %td= contributor.name
      %td= contributor.email
